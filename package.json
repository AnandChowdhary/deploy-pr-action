  "bugs": {
    "url": "https://github.com/koj-co/deploy-pr-action/issues"
  },
  "homepage": "https://github.com/koj-co/deploy-pr-action#readme",
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "@types/fs-extra": "^9.0.1",
    "@types/jest": "^26.0.10",
